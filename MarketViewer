    private void ConnectActionPerformed(java.awt.event.ActionEvent evt) {                                        
        try {
            askbars =  new AskBars();
        } catch (InterruptedException ex) {
            Logger.getLogger(MarketViewer.class.getName()).log(Level.SEVERE, null, ex);
        }
        try {        
            bidbars =  new BidBars();
        } catch (InterruptedException ex) {
            Logger.getLogger(MarketViewer.class.getName()).log(Level.SEVERE, null, ex);
        }
    try { 
        db = new DataBaseDina();
        this.db.selectmarket();
    } catch (SQLException ex) {
        Logger.getLogger(MarketViewer.class.getName()).log(Level.SEVERE, null, ex);
    } catch (ClassNotFoundException ex) {
        Logger.getLogger(MarketViewer.class.getName()).log(Level.SEVERE, null, ex);
    }  
    try {
        Thread.sleep(1000);
    } catch (InterruptedException ex) {
        Logger.getLogger(MarketViewer.class.getName()).log(Level.SEVERE, null, ex);
    }
    System.out.println("hola");
    askprice = this.db.Stringaskprice;
    bidprice = this.db.Stringbidprice;
    System.out.println(askprice);  
    }                                       


