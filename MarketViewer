    public MarketViewer(JTextField ASKPercentage, JTextField BIDPercentage, JButton Connect, JTextField DASK, JTextField DAskSize, JTextField DBID, JTextField DBidSize, JTextField StockASK, JTextField StockAskSize, JTextField StockBID, JTextField StockBidSize, JLabel jLabel1, JLabel jLabel10, JLabel jLabel11, JLabel jLabel12, JLabel jLabel2, JLabel jLabel3, JLabel jLabel4, JLabel jLabel5, JLabel jLabel6, JLabel jLabel7, JLabel jLabel8, JLabel jLabel9, JPanel jPanel1, BidBars bidbar, AskBars askbar, double bidPrice, double askPrice) throws HeadlessException {
        this.ASKPercentage = ASKPercentage;
        this.BIDPercentage = BIDPercentage;
        this.Connect = Connect;
        this.DASK = DASK;
        this.DAskSize = DAskSize;
        this.DBID = DBID;
        this.DBidSize = DBidSize;
        this.StockASK = StockASK;
        this.StockAskSize = StockAskSize;
        this.StockBID = StockBID;
        this.StockBidSize = StockBidSize;
        this.jLabel1 = jLabel1;
        this.jLabel10 = jLabel10;
        this.jLabel11 = jLabel11;
        this.jLabel12 = jLabel12;
        this.jLabel2 = jLabel2;
        this.jLabel3 = jLabel3;
        this.jLabel4 = jLabel4;
        this.jLabel5 = jLabel5;
        this.jLabel6 = jLabel6;
        this.jLabel7 = jLabel7;
        this.jLabel8 = jLabel8;
        this.jLabel9 = jLabel9;
        this.jPanel1 = jPanel1;
        this.bidPrice = bidPrice;
        this.askPrice = askPrice;
    }

    public MarketViewer(JTextField ASKPercentage, JTextField BIDPercentage, JButton Connect, JTextField DASK, JTextField DAskSize, JTextField DBID, JTextField DBidSize, JTextField StockASK, JTextField StockAskSize, JTextField StockBID, JTextField StockBidSize, JLabel jLabel1, JLabel jLabel10, JLabel jLabel11, JLabel jLabel12, JLabel jLabel2, JLabel jLabel3, JLabel jLabel4, JLabel jLabel5, JLabel jLabel6, JLabel jLabel7, JLabel jLabel8, JLabel jLabel9, JPanel jPanel1, BidBars bidbar, AskBars askbar, double bidPrice, double askPrice, GraphicsConfiguration gc) {
        super(gc);
        this.ASKPercentage = ASKPercentage;
        this.BIDPercentage = BIDPercentage;
        this.Connect = Connect;
        this.DASK = DASK;
        this.DAskSize = DAskSize;
        this.DBID = DBID;
        this.DBidSize = DBidSize;
        this.StockASK = StockASK;
        this.StockAskSize = StockAskSize;
        this.StockBID = StockBID;
        this.StockBidSize = StockBidSize;
        this.jLabel1 = jLabel1;
        this.jLabel10 = jLabel10;
        this.jLabel11 = jLabel11;
        this.jLabel12 = jLabel12;
        this.jLabel2 = jLabel2;
        this.jLabel3 = jLabel3;
        this.jLabel4 = jLabel4;
        this.jLabel5 = jLabel5;
        this.jLabel6 = jLabel6;
        this.jLabel7 = jLabel7;
        this.jLabel8 = jLabel8;
        this.jLabel9 = jLabel9;
        this.jPanel1 = jPanel1;
        this.bidPrice = bidPrice;
        this.askPrice = askPrice;
    }

